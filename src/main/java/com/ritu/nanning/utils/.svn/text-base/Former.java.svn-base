package com.ritu.nanning.utils;

public class Former {

	public static float toFloat(String s) {
		try {
			return Float.parseFloat(s);
		} catch (Exception e) {
			return 0f;
		}
	}

	public static String toString(float f) {
		try {
			return Float.toString(f);
		} catch (Exception e) {
			return "0";
		}
	}
}
