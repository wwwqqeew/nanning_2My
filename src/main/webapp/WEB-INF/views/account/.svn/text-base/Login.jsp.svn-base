<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<link type="text/css" rel="stylesheet" href="<%=request.getContextPath()%>/static/styles/login.css" />
<link rel="icon" href="<%=request.getContextPath()%>/static/images/favicon.ico">
<script type="text/javascript" src="<%=request.getContextPath()%>/static/jquery/jquery-1.9.1.min.js"></script>
<script type="text/javascript" src="<%=request.getContextPath()%>/static/jquery-validation/checkOnKepDown.js"></script>

<title>2交通信号干预控制软件后台管理系统-登录</title>
</head>
<body>
	<div class='bg'>
	<div class='bg_minXY'>
		<div class='header'></div>
			<div class='content'>
				<form class='loginForm' action="" method="post" accept-charset="utf-8">
					<input type="text" class="username" name="username" value="admin" />
					<input type="password" class="password" name="password" value="admin" />
					<div class="VerifyCode-div"><img id="VerifyCode"  alt="验证码" src="verifycode" onclick="changeValidateCode(this)" style="cursor: pointer;height: 26px;"/></div>
						<input type="text" class="verifycode" name="verifycode" />
						<input class="submit_btn"  type="button"  />
						<input  type="reset" class="submit_btn_cz"  value=""/>
				</form>
			</div>
			<div class='footer'>技术支持：公安部交通管理科学研究所&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
		</div>
	</div>
</body>
<script type="text/javascript" src="<%=request.getContextPath()%>/static/app/account/login.js"></script>
</html>
