/* global */
html,body {
	margin: 0;
	padding: 0;
	font-size: 14px;
	color: #7d7d7d;
	font-family: monospace;
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.main-content {
  background-color: #52c3f1;
  height: 7px;
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 2;
}

/**map-start*/
.shade-map {
  background-color: #0000FF;
  background-image: url("../images/bg-main.png");
  height: 37px;
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 2;
}

.main-content-map{
	position: absolute;
	/*
	width: 100%;
	height: 100%;
	*/
	top: 52px;
	left: 15px;
	background: #ffffff;
	border: 2px #CCCCCC solid;
}

.main-content-top {
  background: none repeat scroll 0 0 #FFFFFF;
  border-bottom: 2px solid #CCCCCC;
  height: 2px;
  left: 0;
  position: absolute;
  top: -2px;
  width: 100%;
}
.main-content-bottom {
  background: none repeat scroll 0 0 #FFFFFF;
  border-top: 2px solid #CCCCCC;
  bottom: -2px;
  height: 2px;
  left: 0;
  position: absolute;
  width: 100%;
}
.main-content-left {
  background: none repeat scroll 0 0 #FFFFFF;
  border-right: 2px solid #CCCCCC;
  top: 0px;
  width: 2px;
  left: -2px;
  position: absolute;
  height: 100%;
}
.main-content-right {
  background: none repeat scroll 0 0 #FFFFFF;
  border-left: 2px solid #CCCCCC;
  top: 0px;
  width: 2px;
  right: -2px;
  position: absolute;
  height: 100%;
}
.main-content-top-left-corner {
  background: none repeat scroll 0 0 #FFFFFF;
  height: 2px;
  left: -2px;
  position: absolute;
  top: -2px;
  width: 2px;
}
.main-content-top-right-corner {
  background: none repeat scroll 0 0 #FFFFFF;
  height: 2px;
  right: -2px;
  position: absolute;
  top: -2px;
  width: 2px;
}
.main-content-bottom-left-corner {
  background: none repeat scroll 0 0 #FFFFFF;
  height: 2px;
  left: -2px;
  position: absolute;
  bottom: -2px;
  width: 2px;
}
.main-content-bottom-right-corner {
  background: none repeat scroll 0 0 #FFFFFF;
  height: 2px;
  right: -2px;
  position: absolute;
  bottom: -2px;
  width: 2px;
}
.hiddentMap{
	z-index: -1;!importment;
}
/**map-end**/
.noEmpty{
	color: #FF0000;
}
.hiddent {
