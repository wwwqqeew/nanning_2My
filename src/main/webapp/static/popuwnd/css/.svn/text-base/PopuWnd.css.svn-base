.shadow-top {
	position: absolute;
	top: 0px;
	left: 5px;
	height: 5px;
	background-position: bottom;
	background-repeat: repeat-x;
	background-image: url(../images/popuwnd-top.png) !important;
	_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='./static/popuwnd/images/popuwnd-top.png',
		sizingMethod='scale');
	_background-image: none;
	overflow: hidden;
}

