.popuwnd {
	position: absolute;
	z-index: 10000;
	overflow: hidden;
}

.popuwnd .popuwnd-main {
	position: absolute;
	background: #ffffff;
	z-index: 10001;
	top: 5px;
	left: 5px;
	width: 100%;
	height: 100%;
	border: 1px #75BBDD solid;
	border-top: 1px #75BBDD solid;
	overflow: hidden;
	width: 100%;
}

.popuwnd .popuwnd-title {
  background-color: #EBF8FD;
  border-bottom: 1px solid #afcde3;
  height: 35px;
  position: relative;
  width: 100%;
  z-index: 10000;
}

.popuwnd .popuwnd-title .popuwnd-title-tab {
	position: absolute;
	margin: 10px 50px 0px 15px;
	height: 16px;
	color: #2363a1;
	font-family: monospace;
	font-size: 14px;
	font-weight: bold;
	overflow: hidden;
	
}

.popuwnd .popuwnd-title .popuwnd-title-del-btn {
	position: absolute;
	top: 9px;
	right: 9px;
	width: 17px;
	height: 17px;
	_width: 19px;
	_height: 19px;
	border: 0px #e5e5e5 solid;
	background-image: url("../images/popuwnd-close.png") !important;
	_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='./static/popuwnd/images/popuwnd-close.png',
		sizingMethod='scale');
	_background-image: none;
	
	cursor: pointer;
	z-index: 10005;
}

.popuwnd .popuwnd-title .popuwnd-title-maxsize-btn {
	position: absolute;
	top: 8px;
	right: 30px;
	width: 17px;
	height: 12px;
	_width: 19px;
	_height: 19px;
	border: 1px #e5e5e5 solid;
	border-top: 6px #e5e5e5 solid;
	cursor: default;
	z-index: 10005;
}

.popuwnd .popuwnd-title .popuwnd-title-show {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	z-index: 10001;
	cursor: move;

}

.popuwnd .popuwnd-container {
	z-index: 10000;
	position: absolute;
	width: 100%;
}

.popuwnd .popuwnd-container iframe {
	position: relative;
	width: 100%;
	height: 100%;
	border: none;
}

#popuwnd-backgroung {
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 9999;
	width: 100%;
	height: 100%;
	background-image: url("../images/popuwnd-background.png") !important;
	_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='./static/popuwnd/images/popuwnd-background.png',
		sizingMethod='scale');
	_background-image: none;

}

