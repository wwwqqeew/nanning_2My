$(document).ready(function(){ 

	$(".bigImg").mouseover(function(){
		
		var img = $(this).get(0);
		addEvent(img, 'mousewheel', function(e) {
			stopEvent(e);
			var delta = getWheelValue(e);
			//每次递增（或递减）10px，使用了范围限制，保证图片不会过大过小 
			img.style.width = range(img.offsetWidth + (delta * 10), 10000, 100) + 'px';
		//	img.style.height = range(img.offsetHeight + (delta * 10), 8000, 300) + 'px';
			return false;
		});
	})
